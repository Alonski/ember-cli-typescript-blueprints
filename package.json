{
  "name": "ember-cli-typescript-blueprints",
  "version": "1.0.0",
  "description": "TypeScript blueprints for generating ember-cli entities",
  "keywords": [
    "ember-addon",
    "typescript"
  ],
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/typed-ember/ember-cli-typescript-blueprints.git",
  "bugs": {
    "url": "https://github.com/typed-ember/ember-cli-typescript-blueprints"
  },
  "homepage": "https://github.com/typed-ember/ember-cli-typescript-blueprints",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "mocha --recursive node-tests",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "ember-cli-get-component-path-option": "^1.0.0",
